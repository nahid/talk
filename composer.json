{
    "name": "nahid/talk",
    "description": "Talk is a PHP based users conversations library. It helps to develop users messages and conversations in easy way. Talk also support Laravel 5>=",
    "keywords": ["message", "inbox", "conversations", "chat", "php", "laravel"],
    "homepage": "https://github.com/nahid/talk",
    "type": "library",
    "license": "CC(3.0)",
    "authors": [
        {
            "name": "Nahid Bin Azhar",
            "email": "nahid.dns@gmail.com"
        }
    ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/contracts": "5.0.* || 5.1.* || 5.2.* || 5.3.*",
    "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.*",
    "sebastian-berc/repositories": "^1.0",
    "nesbot/carbon": "^1.21"
  },
  "require-dev": {
    "graham-campbell/testbench": "^3.1",
    "phpunit/phpunit": "^4.8 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Nahid\\Talk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nahid\\Talk\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
